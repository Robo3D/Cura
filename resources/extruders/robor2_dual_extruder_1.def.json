{
    "id": "robor2_dual_extruder_1",
    "version": 2,
    "name": "Right Extruder",
    "inherits": "fdmextruder",
    "metadata": {
        "machine": "robor2_dual",
        "position": "1"
    },

    "overrides": {
        "extruder_nr": {
            "default_value": 1,
            "maximum_value": "1"
        },
        "machine_nozzle_offset_x": { "default_value": 21.5 },
        "machine_nozzle_offset_y": { "default_value": 0 },
        "machine_extruder_start_code": {
          "default_value": "\nG1 X({prime_tower_position_x} + ({prime_tower_size} / 2)) Y({prime_tower_position_y} + ({prime_tower_size} / 2)) ;move for wipe\nG91 ;relative coordinates\nG1 X-15 ;wipe\nG1 X{prime_tower_size} ;wipe\nG1 X-({prime_tower_size} + 10) ;wipe\nG1 X({prime_tower_size} + 10) ;wipe\nG1 X-({prime_tower_size} + 10) ;wipe\nG1 X({prime_tower_size} + 10) ;wipe\nG90 ;absolute coordiates\n\n"
        },
        "machine_extruder_end_code": {
           "default_value": "\nM104 T1 S{material_standby_temperature} ; cool extruder\nM109 T3 R{material_standby_temperature}; wait for temp\n\n;end T3\n\n"
         },

        "machine_extruder_start_pos_abs": { "default_value": true },
        "machine_extruder_start_pos_x": { "value": "prime_tower_position_x" },
        "machine_extruder_start_pos_y": { "value": "prime_tower_position_y" },
        "machine_extruder_end_pos_abs": { "default_value": true },
        "machine_extruder_end_pos_x": { "value": "prime_tower_position_x" },
        "machine_extruder_end_pos_y": { "value": "prime_tower_position_y" },
        "retraction_amount_multiplier": { "default_value": 2 },
        "retraction_amount": { "value": "retraction_amount + 2" },
        "switch_extruder_retraction_speeds": { "default_value": 20 },
        "extruder_prime_pos_x": { "default_value": 197 },
        "extruder_prime_pos_y": { "default_value": 0 },
        "extruder_prime_pos_z": { "default_value": 2 }
    }
}
